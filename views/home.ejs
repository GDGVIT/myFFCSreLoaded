<script type="text/javascript">
  $('.submit-button').click(function(){
    if($('.login-form #login-regnum').val().length>0 && $('.login-form #login-pass').val().length>0){
      if(/[0-9]{2}[A-Za-z]{3}[0-9]{4}/g.test($('#login-regnum').val())){
        if($('#login-pass').val().length>=6){
          $('#modal1').openModal();
          $('.create-new').click(function(){
            $('.login-form').submit();
          })
        }
      } else{
        $("#login-regnum").addClass("invalid");
        $("#login-regnum").prop("aria-invalid", "true");
        return false;;

      }
    }
    if($('#reg-name').val().length>0 && $('#reg-regnum').length>0 && $('#reg-pass').val().length>0){
      if(/[0-9]{2}[A-Za-z]{3}[0-9]{4}/g.test($('#reg-regnum').val())){
        if($('#reg-name').length>0){
          if($("#reg-pass").val().length>=6){
            $('.register-form').submit();
          } else{
              //Password Length Error
              $("#reg-pass").addClass("invalid");
              $("#reg-pass").prop("aria-invalid", "true");
              return false;;
            }
          } else{
            $("#reg-name").addClass("invalid");
            $("#reg-name").prop("aria-invalid", "true");
            return false;;
          }
        } else{
          $("#reg-regnum").addClass("invalid");
          $("#reg-regnum").prop("aria-invalid", "true");
          return false;;
        }
      }
    });
  </script>
