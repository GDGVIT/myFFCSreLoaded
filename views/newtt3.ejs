  <script>
  function doMark(){
	var sll = "<%=slots%>"
    var arr= sll.split(',');
    for(var i=0;i<arr.length;i++){
      $("."+arr[i]).addClass('selected').removeClass('unselected').text(arr[i])
    }
    window.callPhantom('takeShot');
  }
  window.addEventListener('load',function(){
		doMark();
  })
  </script>
