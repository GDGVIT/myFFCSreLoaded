        function close_side(){
         state=0;
         $(".hamburger").removeClass("is-active");
         $(".side-nav-my").removeClass("opened");
       }
       $(".button-collapse").click(function(){
        if(state==0)
        {
          open_side();
        }
        else
        {
          close_side();
        }
      });
    </script>
